{
    "base": {
        "dir": "../data"
    },
    "retrieve": {
        "hvConfigRegex": ".*([0-9a-f]{2}).config.json$",
        "hvs": [
            "http://localhost:8001/oaipmh/harvest",
            "http://localhost:8002/oaipmh/harvest"
        ],
        "hvUnloadSrc": [
            "46f48f6a832e",
            "6505543f78fe"
        ],
        "hvUnloadCmd": "docker cp {}:/var/lib/jetty/cache/records/OaiPmhETL.json {}",
        "hvRangeTo": 100000
    },
    "clean": {
        "retrieveHash": "0797f318a1b1d0be80fa2c224d8678b80e76c7f951b4c29a83b25eeca369fce5",
        "mappingHash": "2cc9765ec445d0eee3728abe4e20e505ed02f7329010c5cbb82893a4b75019ed",
        "lang": "en",
        "payloadFields": [
            "titles",
            "descriptions"
        ],
	"payloadMinLength": 200,
        "regex": {
            "ddcValue": "(^\\d+\\.\\d+,)+",
            "ddcSchemeURI": "https{0,1}://dewey.info/*",
            "special": "f[0-9a-f]{1}\\.json",
            "dataInput": ".*([a-f0-9]{2})\\.json",
            "dataOutput": ".*[a-f0-9]{2}\\.chunk\\.json"
        },
        "schemes": [
            "anzsrc",
            "ddc",
            "bk",
            "narcis",
            "bepress",
            "linsearch"
        ],
        "special": {
            "f1.json": 19,
            "f2.json": 17,
            "f3.json": 6,
            "f4.json": 19,
            "f5.json": 19,
            "f6.json": 19,
            "f7.json": 17,
            "f8.json": 17,
            "f9.json": 17,
            "fa.json": 17,
            "fb.json": 20,
            "fc.json": 20,
            "fd.json": 16,
            "fe.json": 16,
            "ff.json": 16
        }
    },
    "vectorize": {
        "cleanHash": "9ee9ce89f8ae4b639fc899ad5765635cb3af5bffad88736c7d6bdc70c21d65e9",
        "stemming": "lancaster",
        "test_size": 0.2,
        "minDocFreq": 2,
        "ngramRange": [1,2],
        "tokenMode": "word",
        "feature_selection": {
            "mode": "multipleOfLabels",
            "value": 1000
        },
        "stopWordsHash": "fdb7caa2414fdeb0b1e208b637e1fea44704dfc105573eb4357468ec8ed4dd60"
    },
    "evaluate": {
    	"cv": 10,
	"models": [
		{
		    "name": "MLPClassifier",
		    "package": "sklearn.neural_network",
		    "multilabel": "true",
		    "params": {
			    "n_iter_no_change": 3,
			    "batch_size": 1000,
			    "hidden_layer_sizes": [100,100]
		    },
		    "param_grid": [
			{
			    "activation": ["relu", "tanh"], 
			    "solver": ["adam", "sgd"]
			}
		    ]
		}, 
		{
		    "name": "RidgeClassifier",
		    "package": "sklearn.linear_model",
		    "multilabel": "false",
		    "params": { },
		    "param_grid": [
			{
			    "alpha": [0.5, 1.0, 2.0, 5.0,100.0], 
			    "solver": ["sag"]
			}
		    ]
		},
		{
		    "name": "XGBClassifier",
		    "package": "xgboost",
		    "multilabel": "false",
		    "params": { 
		    	"objective": "binary:logistic",
			"verbosity": 3,
			"booster": "gbtree"
		    },
		    "param_grid": [
			{
			    "max_depth": [300, 600, 1200]
			}
		    ]
		},
		{
		    "name": "RandomForestClassifier",
		    "package": "sklearn.ensemble",
		    "multilabel": true,
		    "params": {
			    "n_estimators": 100,
			    "verbose": 10
	            },
		    "param_grid": [
			{
			    "max_depth": [300, 600, 1200], 
			    "min_samples_split": [2,10,20] 
			}
		    ]
		}
	]
    }
}

