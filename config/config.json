{
    "base": {
        "dir": "../data",
        "categories": "anzsrc.json",
        "dtimestamp": "2019-05-19",
        "seed": 1234
    },
    "retrieve": {
        "hvConfigRegex": ".*([0-9a-f]{2}).config.json$",
        "hvs": [
            "http://localhost:8001/oaipmh/harvest",
            "http://localhost:8002/oaipmh/harvest"
        ],
        "hvUnloadSrc": [
            "3dfd10fd6098",
            "780a8e742637"
        ],
        "hvUnloadCmd": "docker cp {}:/var/lib/jetty/cache/records/OaiPmhETL.json {}",
        "hvRangeTo": 10
    },
    "clean": {
        "dmode": "title_description",
        "dregex": ".*[a-f0-9]{2}\\.json",
        "sregex": "f[0-9a-f]{1}\\.json"
    },
    "sample": {
        "ratio1": 0.8,
        "ratio2": 0.9
    },
    "vectorize": {
        "minDocFreq": 2,
        "ngramRange": [1,2],
        "tokenMode": "word",
        "topK": 20000
    },
    "train": {
        "mlp": {
            "dtype": "dmed",
            "batchSize": 128,
            "dropoutRate": 0.2,
            "epochs": 1000,
            "layers": 2,
            "learningRate": 1e-3,
            "units": 64
        }
    }
}
